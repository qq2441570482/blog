<div class="col-md-3">
  <div class="panel panel-default paneltags">
      <div id="example" style="height: 300px"></div>
  </div>
</div>

<script type="text/javascript">
  var word_list = [
    <% @tags.each do |tag| %>
      {text: "<%= tag.name %>", weight: Math.floor((Math.random() * 10) + 1)},
    <% end %>
  ];

  $(document).ready(function () {
    $('#example').jQCloud(word_list)
  })
</script>